{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Ember Sherpa",
    "owner": "Taras Mankovski",
    "description": "Ember.js Cheatsheet and Development Companion"
  },
  "plugins": [
    "./node_modules/wintersmith-less",
    "./plugins/onepager",
    "./plugins/codemodule",
    "./plugins/shortcodes",
    "./plugins/codemodule-shortcode",
    "./plugins/typeahead"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "sanitize": false
  }
}