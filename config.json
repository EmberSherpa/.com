{
  "locals": {
    "url": "http://embersherpa.com",
    "dev": "http://localhost:2777",
    "name": "Ember Sherpa",
    "owner": "Taras Mankovski",
    "description": "Ember.js Cheatsheet and Development Companion"
  },
  "plugins": [
    "./node_modules/wintersmith-browserify",
    "./node_modules/wintersmith-less",
    "./plugins/helpers",
    "./plugins/cheatsheet",
    "./plugins/shortcodes",
    "./plugins/shortcodes/dialogs",
    "./plugins/shortcodes/headings",    
    "./plugins/typeahead",
    "./plugins/env-patch"
  ],
  "port": "2777",
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "sanitize": false,
    "gfm": true,
    "tables": true,
    "breaks": true
  },
  "repository": {
    "base": "https://github.com/taras/embersherpa"
  },
  "codemodule": {
    "pattern": "cheatsheet/**/*.coffee",
    "template": "test.jade"
  }
}