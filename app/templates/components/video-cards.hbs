{{#if videos}}
  {{#each videos as |video|}}
    {{#video-card activator=true video=video}}
      {{#link-to 'global-ember-meetup.video' video (query-params autoplay=true)}}
        <i class="fa fa-play-circle"></i> Play
      {{/link-to}}
      {{disqus-comment-count
        identifier=video.id
        title=video.name
      }}
    {{/video-card}}
  {{/each}}
{{else}}
  {{md-loader mode="circular"}}
{{/if}}
